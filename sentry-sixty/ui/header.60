export Header := VerticalLayout {
    vertical_stretch: 0;
    padding: 32px;
    spacing: 4px;
    property title <=> title_el.text;
    property subtitle <=> subtitle_el.text;

    title_el := Text {
        font_size: 22px;
        color: #2B1D38;
    }

    subtitle_el := Text {
        font_size: 14px;
        color: #776589;
    }
}
